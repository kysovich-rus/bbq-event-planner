<%= form_for [@event, photo], html: { multipart: true }, data: { turbo: false } do |f| %>
  <%= render partial: 'partials/form_errors', locals: { model: photo } %>

  <div class="form-group">
    <p><%= f.label :photo %></p>
    <p><%= f.file_field :photo, class: 'form-control' %></p>
  </div>

  <div class="actions">
    <%= f.submit 'Загрузить', class: 'btn btn-dark' %>
  </div>
<% end %>